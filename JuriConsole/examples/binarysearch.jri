fun binary_search :arr low high x

    if high >= low
 
        mid = (high + low)//2

        if mid:arr == x
            return mid
        
        if mid:arr > x
            return binary_search(:arr low mid-1 x)
 
        if mid:arr <= x
            return binary_search(:arr mid+1 high x)
 
    
    return -1

:arr = [0 to 100]
x = 11 
result = binary_search(:arr 0 (?:arr)-1 x)

print(result)
